{
  "name": "mdcu-parser",
  "version": "1.0.0",
  "description": "mdcu parser for scrap books list",
  "main": "index.js",
  "scripts": {
    "parse": "node src/parser.js",
    "web": "./node_modules/.bin/http-server public --port 8000",
    "api": "node src/server.js"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.5",
    "http-server": "^0.12.3",
    "moment": "^2.29.1",
    "mongoose": "^5.11.19",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "restify": "^8.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "prettier": "^2.2.1"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "semi": false
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "prettier",
      "plugin:import/errors"
    ],
    "plugins": [
      "prettier",
      "import"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "arrowParens": "always",
          "semi": false,
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ],
      "no-irregular-whitespace": 0
    },
    "env": {
      "commonjs": true,
      "es6": true,
      "jest": true,
      "node": true
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Pixy/mdcu-parser.git"
  },
  "author": "Pierre-Alexis GODET",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Pixy/mdcu-parser/issues"
  },
  "homepage": "https://github.com/Pixy/mdcu-parser#readme"
}
